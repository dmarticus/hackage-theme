      "scope": "support.function.be.latex",
      "settings": {
        "foreground": "#ea6962"
      }
    },
    {
      "name": "LaTex orange",
      "scope": "support.function.section.latex, keyword.control.table.cell.latex, keyword.control.table.newline.latex",
      "settings": {
        "foreground": "#e78a4e"
