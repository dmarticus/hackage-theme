  "tokenColors": [
    // Basic source code color
    {
      "scope": [
        "source",
      ],
      "settings": {
        "foreground": "#073642"
      }
    },
    // Comments
    {
      "scope": [
        "comment",
        "punctuation.whitespace.comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#8a8a8a",
        "fontStyle": ""
      }
    },
    // Punctuation
    {
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter",
        "meta.bracket"
      ],
      "settings": {
        "foreground": "#dc322f"
      }
    },
    // Constants
    {
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#073642",
      }
    },
    {
      "scope": [
        "constant",
        "variable.other.member",
        "meta.record punctuation.brace",
        "keyword.operator.lambda",
        "keyword.operator.colon"
      ],
      "settings": {
        "foreground": "#5f5faf",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "string.quoted",
        "punctuation.definition.string",
        "keyword.operator.prefix.minus"
      ],
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "scope": [
        "constant.character.escape",
        "punctuation.quasi.element",
        "punctuation.definition.template-expression",
        "punctuation.section.embedded",
        "storage.type.format",
        "constant.other.placeholder",
        "variable.interpolation",
        "meta.quasi-quotation",
      ],
      "settings": {
        "foreground": "#073642"
      }
    },
    // Keywords
    {
      "scope": [
        "keyword",
        "modifier",
        "keyword.operator.bind",
        "punctuation.separator.key-value.html",
        "meta.tag",
        "keyword.control",
        "keyword.operator",
        "constant.other.color",
        "punctuation.definition.constant",
        "meta.function.block.start.handlebars",
        "meta.function.inline.other",
        "meta.property-value",
        "support.constant.mathematical-symbols",
        "support.constant.vendored.property-value",
        "punctuation.definition.keyword",
        "punctuation.accessor",
        "keyword.operator.promotion",
        "keyword.operator.function.infix",
        "punctuation.separator.property"
      ],
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "scope": "keyword.other.debugger",
      "settings": {
        "foreground": "#EA6279"
      }
    },
    {
      "scope": [
        "keyword.operator.double-colon",
        "keyword.operator.arrow",
        "keyword.operator.big-arrow",
        "keyword.operator.period",
        "keyword.operator.eq",
        "keyword.operator.apply",
        "keyword.operator.pipe",
        "keyword.operator.double-dot",
        "keyword.operator.lambda",
        "keyword.operator.prefix",
        "punctuation.backtick"
      ],
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "scope": [
        "keyword.operator.prefix.at"
      ],
      "settings": {
        "foreground": "#B3DDF5",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "keyword.operator.prefix.hash",
        "entity.name.label",
        "constant.other.label",
      ],
      "settings": {
        "foreground": "#8BBBC2",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "keyword.operator.postfix.hash",
        "keyword.operator.hash"
      ],
      "settings": {
        "foreground": "#9099A7",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "keyword.operator.period"
      ],
      "settings": {
        "foreground": "#dc322f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.other.forall"
      ],
      "settings": {
        "foreground": "#d33682",
      }
    },
    // Invalid code
    {
      "scope": [
        "invalid",
        "token.error-token"
      ],
      "settings": {
        "foreground": "#AA2A2A",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "support.type",
        "support.class.console",
        "keyword.other.debugger",
        "entity.other.inherited-class",
        "meta.property-name",
        "punctuation.definition.raw.markdown"
      ],
      "settings": {
        "foreground": "#073642"
      }
    },
    // Types
    {
      "scope": [
        "variable.parameter",
        "text.html",
        "punctuation.section.property-list",
        "meta.property-value.scss punctuation",
        "meta.property-list",
        "keyword.operator.type.annotation"
      ],
      "settings": {
        "foreground": "#5f5faf"
      }
    },
    // Variables
    // {
    //   "scope": [
    //     "variable",
    //     "support.variable",
    //     "meta.definition.variable"
    //   ],
    //   "settings": {
    //     "foreground": "#5f5faf"
    //   }
    // },
    {
      "scope": [
        "variable.language",
        "variable.other"
      ],
      "settings": {
        "foreground": "#073642"
      }
    },
    {
      "name": "Variable start",
      "scope": "punctuation.definition.variable",
      "settings": {
        "foreground": "#859900"
      }
    },
    // Namespacing
    {
      "scope": [
        "entity.name",
        "entity.other.attribute-name",
        "meta.function.type-declaration",
        "meta.other.unknown",
        "punctuation.backtick",
        "punctuation.decorator",
      ],
      "settings": {
        "foreground": "#073642"
      }
    },
    {
      "scope": [
        "meta.import",
        "entity.name.namespace",
        "meta.declaration.module",
        "support.module",
        "support.node",
        "support.other.module",
        "support.type.object.module",
        "entity.name.type.module",
        "entity.name.type.class.module",
        "keyword.control.module"
      ],
      "settings": {
        "foreground": "#073642"
      }
    },
    // Functions
    // {
    //   "scope": [
    //     "entity.name.function"
    //   ],
    //   "settings": {
    //     "foreground": "#5f5faf"
    //   }
    // },
    {
      "scope": [
        "support.function",
        "variable.function.constructor",
        "meta.function",
        "meta.function-call",
        "meta.definition.method"
      ],
      "settings": {
        "foreground": "#073642"
      }
    },
    // Preprocessor
    {
      "scope": [
        "meta.preprocessor",
        "keyword.other.preprocessor",
        "punctuation.definition.preprocessor",
        "entity.name.function.preprocessor",
      ],
      "settings": {
        "foreground": "#2aa198"
      }
    },
    {
      "scope": [
        "keyword.other.preprocessor.extension"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "keyword.other.preprocessor"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    // Classes (OO)
    {
      "scope": "entity.name.type.class, support.class, entity.name.class, entity.other.inherited-class, storage.class",
      "settings": {
        "foreground": "#7DAEA3"
      }
    },
    // Markdown, LaTeX and other formatting (taken from Gruvbox Material)
    {
      "name": "Markdown heading1",
      "scope": "heading.1.markdown, markup.heading.setext.1.markdown",
      "settings": {
        "foreground": "#ea6962",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown heading2",
      "scope": "heading.2.markdown, markup.heading.setext.2.markdown",
      "settings": {
        "foreground": "#e78a4e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown heading3",
      "scope": "heading.3.markdown",
      "settings": {
        "foreground": "#d8a657",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown heading4",
      "scope": "heading.4.markdown",
      "settings": {
        "foreground": "#a9b665",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown heading5",
      "scope": "heading.5.markdown",
      "settings": {
        "foreground": "#7daea3",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown heading6",
      "scope": "heading.6.markdown",
      "settings": {
        "foreground": "#d3869b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown heading delimiter",
      "scope": "punctuation.definition.heading.markdown",
      "settings": {
        "foreground": "#928374",
        "fontStyle": "regular"
      }
    },
    {
      "name": "Markdown link",
      "scope": "string.other.link.title.markdown, constant.other.reference.link.markdown, string.other.link.description.markdown",
      "settings": {
        "foreground": "#d3869b",
        "fontStyle": "regular"
